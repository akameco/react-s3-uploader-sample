{
  "name": "react-s3-image-drop-upload",
  "version": "0.0.1",
  "description": "react + s3 + axios sample app",
  "main": "index.js",
  "scripts": {
    "start": "node main.js",
    "build": "browserify -t babelify index.js -o ./public/bundle.js",
    "watch": "watchify -t babelify index.js -o ./public/bundle.js",
    "test": "xo"
  },
  "author": "akameco <akameco.t@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.2.41",
    "axios": "^0.9.1",
    "express": "^4.13.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "watchify": "^3.7.0",
    "xo": "*"
  },
  "xo": {
    "space": true,
    "envs": ["node", "browser"],
    "esnext": true,
    "overrides": [
      {
        "files": "main.js",
        "esnext": false
      }
    ]
  }
}
